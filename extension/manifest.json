{

  "manifest_version": 2,
  "name": "Borderify2",
  "version": "1.0",

  "description": "TUTORIAL Adds a red border to all webpages matching mozilla.org.",

  "icons": {
    "48": "icons/border-48.png"
  },

  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["prefetch_extension.js"]
    }
  ],
  "background": {
    "scripts": ["background.js"]
  },
  "permissions": [
    "dns",
    "notifications"
  ],
  "applications": {
    "gecko": {
      "id": "borderify@example.com"
    }
  }
}
