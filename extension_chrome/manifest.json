 {
    "name": "prefetch_chrome",
    "version": "2",
    "description": "COS 561 project for prefetching implementation for chrome.",
    "manifest_version": 2,
    "icons": {
      "48": "border-48.png"
    },
    "content_scripts":[
      {
        "matches": ["<all_urls>"],
        "js":["content_script.js"]
      }
    ],
    "background": {
      "persistent": false,
      "scripts": ["background.js"]
    },
    "permissions": [
      "https://*.google.com/", 
      "activeTab"]
  }